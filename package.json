{
  "name": "yandex-disk-task",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "query-string": "^5.1.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "2.0.5",
    "react-table": "^6.8.6",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "build-css build-js",
    "build-js": "react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive --usePolling --polling-interval 500 && npm run build-css",
    "start-js": "react-scripts start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3"
  }
}
